<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/WEB-INF/layouts/">
	<excludes>
		<pattern>/static/*</pattern>
		<pattern>/account/login*</pattern>
		
		<!-- 异步请求 -->
		<pattern>/db/list/*</pattern>
		
		<pattern>/db/user/list/*</pattern>
		<pattern>/db/user/build/*</pattern>
		
		<pattern>/mcluster/build/status/*</pattern>
		<pattern>/mcluster/list/*</pattern>
		
	</excludes>
	
	<decorator name="default" page="default.jsp">
		<pattern>/*</pattern>
	</decorator>
</decorators>
 
